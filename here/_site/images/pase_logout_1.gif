        $('.toggle-icon').on('click', function(e){
            e.stopImmediatePropagation();
            var thisElm = this;
            var elmsDesplegados = $('.subsections-col:visible');

            if($(thisElm).hasClass('open')){
                $(thisElm).removeClass('open');
                $(thisElm).closest('li').find('ul').slideUp(300);
                return false;
            }

            if(elmsDesplegados.length > 0) {
                $('.toggle-icon').removeClass('open');
                elmsDesplegados.slideUp(200, function(){
                    desplegarMenu(thisElm);
                });
            } else {
                desplegarMenu(thisElm);
            }
        });

        function desplegarMenu(elm){
            var classSection = $(elm).attr('class').split(" ")[1];
